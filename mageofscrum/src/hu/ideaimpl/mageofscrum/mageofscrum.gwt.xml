<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.3.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.3.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="mageofscrum">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.activity.Activity"/>
  	<inherits name="com.google.gwt.place.Place"/>
<!--   	<inherits name="com.google.gwt.user.theme.standard.Standard"/> -->
  	<inherits name='com.google.gwt.visualization.Visualization'/>
	<source path="client" />
	<source path="shared" />
	<entry-point class="hu.ideaimpl.mageofscrum.client.MageOfScrum" />

	<set-property name="user.agent" value="safari,gecko1_8" />
<!-- 	<set-property name="user.agent" value="gecko1_8" /> -->
		
	<replace-with class="hu.ideaimpl.mageofscrum.client.ClientFactoryImpl">
		<when-type-is class="hu.ideaimpl.mageofscrum.client.ClientFactory" />
	</replace-with>
</module>